{
  "name": "@kohlmannj/opml-generator",
  "version": "2.0.0-alpha.1",
  "description": "opml generator for node.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:js": "rollup -c rollup.config.js",
    "build:dts": "tsc -p src/tsconfig.json --emitDeclarationOnly",
    "prepublish": "yarn test && yarn build",
    "test": "jest",
    "type-check": "tsc -p src/tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kohlmannj/opml-generator.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "opml",
    "rss",
    "feeds",
    "typescript"
  ],
  "author": "Joseph Kohlmann <kohlmannj@mac.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kohlmannj/opml-generator/issues"
  },
  "homepage": "https://github.com/kohlmannj/opml-generator",
  "peerDependencies": {
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.21",
    "@types/xml": "^1.0.3",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-jest": "^24.9.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.0-beta",
    "xml": "^1.0.1",
    "xml2js": "^0.4.22"
  }
